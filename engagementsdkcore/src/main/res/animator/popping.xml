<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android" android:ordering="together">
    <objectAnimator android:propertyName="scaleX" android:duration="150" android:repeatCount="5"
                    android:valueFrom="1.0"
                    android:repeatMode="reverse"
                    android:valueTo="1.5" android:valueType="floatType" />
    <objectAnimator android:propertyName="scaleY" android:duration="150" android:repeatCount="5"
                    android:valueFrom="1.0"
                    android:repeatMode="reverse"
                    android:valueTo="1.5" android:valueType="floatType" />
</set>